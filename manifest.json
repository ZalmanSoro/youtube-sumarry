{
    "name": "Youtube summary",
    "version": "1.0",
    "description": "Summmaize watchlist duration, based on all open youtube tabs in the browser",
    "author": "Sorotzkin, Zalman",
    "manifest_version": 2,
    "permissions": [
        "tabs",
        "activeTab",
        "https://*.youtube.com/*"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/icon-16.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    }
}